<h3>here you can see your profile deets</h3>
<%= @profile.name %>
<%= @profile.age %>
<%= @profile.location %>
<%= @profile.pronouns %>
<%= @profile.languages %>
<% @profile.photos.each do |photo| %>
  <%= cl_image_tag photo.key, height: 100, width: 100, crop: :fill %>
<% end %>

<%= link_to "home", root_path %>

<%= link_to "delete profile", delete_profile_path(@profile), data: { turbo_method: :delete, turbo_confirm: "Sure you want to delete your profile?" } %>
